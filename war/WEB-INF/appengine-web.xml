<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
	<application>intrade-archive</application>
	<version>11</version>
	
	<!-- Configure java.util.logging -->
	<system-properties>
		<property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
	</system-properties>
	 <static-files> 
  <include path="/**.png" /> 
  <include path="/**.jpg" /> 
  <include path="/**.html" /> 
  <include path="/**.ico" /> 
  </static-files> 
</appengine-web-app>